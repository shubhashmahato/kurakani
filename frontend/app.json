{
  "expo": {
    "name": "Kurakani",
    "slug": "kurakani",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#075E54"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTabletMode": true,
      "bundleIdentifier": "com.kurakani.app",
      "buildNumber": "1"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#075E54"
      },
      "package": "com.kurakani.app",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Kurakani to access your camera"
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow Kurakani to access your photos",
          "savePhotosPermission": "Allow Kurakani to save photos"
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermissions": "Allow Kurakani to access your location"
        }
      ]
    ]
  }
}